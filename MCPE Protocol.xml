<?xml version="1.0" encoding="utf-8"?>

<protocol gameVersion="1.18.30" protocolVersion="503">
	<types>
		<type name="resourcePackIdVersions">
			<field name="resourcePackIdVersions" type="listUVarInt">
				<field name="Id" type="string"/>
				<field name="Version" type="string"/>
				<field name="Subpack Name" type="string"/>
			</field>
		</type>
		<type name="item">
			<field name="Id" type="varint" conditional="!0">
				<field name="Count" type="short" />
				<field name="MetaData" type="uvarint" />
				<field name="Is Unique" type="bool" conditional="True">
					<field name="Unique Id" type="varint" />
				</field>
				<field name="Runtime Id" type="varint" />
				<field name="NBT" type="byteArray" />
			</field>
		</type>
		<type name="metadataDictionary">
			<field name="metadataDictionary" type="listUVarInt" ignoreContainer="true">
				<field name="Index" type="uvarint"/>
				<field name="Metadata Type" type="uvarint" switch="" >
					<field name="Value" type="byte" case="0"/>
					<field name="Value" type="short" case="1"/>
					<field name="Value" type="varint" case="2"/>
					<field name="Value" type="float" case="3"/>
					<field name="Value" type="string" case="4"/>
					<field name="Value" type="nbt" case="5"/>
					<field name="Value" type="posVarint" case="6"/>
					<field name="Value" type="varlong" case="7"/>
					<field name="Value" type="posFloat" case="8"/>
				</field>
			</field>
		</type>
		<type name="posVarint">
			<field name="X" type="varint"/>
			<field name="Y" type="varint"/>
			<field name="Z" type="varint"/>
		</type>
		<type name="posFloat">
			<field name="X" type="float"/>
			<field name="Y" type="float"/>
			<field name="Z" type="float"/>
		</type>
		<type name="entityLinks">
			<field name="entityLinks" type="listUVarInt" ignoreContainer="true">
				<field name="From" type="uvarlong"/>
				<field name="To" type="uvarlong"/>
				<field name="Type" type="byte"/>
				<field name="Immediate" type="bool"/>
				<field name="Caused By Rider" type="bool"/>
			</field>
		</type>
		<type name="transactionRecords">
			<field name="Records" type="listUVarInt">
				<field name="Source Type" type="varint" switch="">
					<field name="Inventory Id" type="varint" case="0"/>
					<field name="Flags" type="uvarint" case="2"/>
					<field name="Action" type="varint" case="100"/>
					<field name="Action" type="varint" case="99999"/>
				</field>
				<field name="Slot" type="uvarint"/>
				<field name="OldItem" type="item"/>
				<field name="NewItem" type="item"/>
			</field>
		</type>
		<type name="normalTransaction">
			<field name="Records" type="transactionRecords" />
		</type>
		<type name="inventoryMismatchTransaction">
			<field name="Records" type="transactionRecords" />
		</type>
		<type name="itemUseTransaction">
			<field name="Records" type="transactionRecords" />
			<field name="Action Type" type="uvarint" />
			<field name="Position" type="posVarint" />
			<field name="Face" type="varint" />
			<field name="Slot" type="varint" />
			<field name="Item" type="item" />
			<field name="From Position" type="posFloat" />
			<field name="Click Position" type="posFloat" />
			<field name="Block Runtime Id" type="uvarint" />
		</type>
		<type name="itemUseOnEntityTransaction">
			<field name="Records" type="transactionRecords" />
			<field name="Entity Id" type="uvarlong" />
			<field name="Action Type" type="uvarint" />
			<field name="Slot" type="varint" />
			<field name="Item" type="item" />
			<field name="From Position" type="posFloat" />
			<field name="Click Position" type="posFloat" />
		</type>
		<type name="itemReleaseTransaction">
			<field name="Records" type="transactionRecords" />
			<field name="Action Type" type="uvarint" />
			<field name="Slot" type="varint" />
			<field name="Item" type="item" />
			<field name="From Position" type="posFloat" />
		</type>
		<type name="transaction">
			<field name="Request Id" type="varint" conditional="!0">
				<field name="Records" type="listUVarInt">
					<field name="Container Id" type="byte"/>
					<field name="Slots" type="listUVarInt">
						<field name="Slot" type="byte"/>
					</field>
				</field>
			</field>
			<field name="Transaction Type" type="varint" switch="">
				<field name="Normal Transaction" type="normalTransaction" ignoreContainer="true" case="0" />
				<field name="Inventory Mismatch Transaction" type="inventoryMismatchTransaction" ignoreContainer="true" case="1"/>
				<field name="Item Use Transaction" type="itemUseTransaction" ignoreContainer="true" case="2"/>
				<field name="Item Use On Entity Transaction" type="itemUseOnEntityTransaction" ignoreContainer="true" case="3"/>
				<field name="Item Release" type="itemReleaseTransaction" ignoreContainer="true" case="4"/>
			</field>
		</type>
	</types>
	<packets>
		<packet id="0x01" name="MCPE_LOGIN">
			<field name="Protocol Version" type="intBE" />
			<field name="Ignored" type="uvarint" />
			<field name="Chain" type="jsonJwtArrayChain" />
			<field name="Client Data" type="jwt" />
		</packet>

		<packet id="0x02" name="MCPE_PLAY_STATUS">
			<field name="Status" type="intBE" />
		</packet>

		<packet id="0x03" name="MCPE_SERVER_TO_CLIENT_HANDSHAKE">
			<field name="Token" type="string" />
		</packet>

		<packet id="0x04" name="MCPE_CLIENT_TO_SERVER_HANDSHAKE">
		</packet>

		<packet id="0x05" name="MCPE_DISCONNECT">
			<field name="Hide disconnect reason" type="bool" />
			<field name="Message" type="string" />
		</packet>

		<packet id="0x06" name="MCPE_RESOURCE_PACKS_INFO">
			<field name="Must accept" type="bool" />
			<field name="Has scripts" type="bool" />
			<field name="Force Server Packs" type="bool" />
			<field name="BehahaviorPackInfos" type="list16">
				<field name="Id" type="string" />
				<field name="Version" type="string" />
				<field name="Size" type="ulong" />
				<field name="Encryption Key" type="string" />
				<field name="Subpack Name" type="string" />
				<field name="Content Id" type="string" />
				<field name="Has scripts" type="bool" />
			</field>
			<field name="TexturePacks" type="list16" >
				<field name="Id" type="string" />
				<field name="Version" type="string" />
				<field name="Size" type="ulong" />
				<field name="Encryption Key" type="string" />
				<field name="Subpack Name" type="string" />
				<field name="Content Id" type="string" />
				<field name="Has scripts" type="bool" />
				<field name="RTX enabled" type="bool" />
			</field>
		</packet>

		<packet id="0x07" name="MCPE_RESOURCE_PACK_STACK">
			<field name="Must accept" type="bool" />
			<field name="BehaviorPackIdVersions" type="resourcePackIdVersions" />
			<field name="ResourcePackIdVersions" type="resourcePackIdVersions" />
			<field name="Game Version" type="string" />
			<field name="Experiments" type="listInt">
				<field name="Name" type="string" />
				<field name="Enabled" type="bool" />
			</field>
			<field name="Experiments Previously Toggled" type="bool" />
		</packet>

		<packet id="0x08" name="MCPE_RESOURCE_PACK_CLIENT_RESPONSE">
			<field name="Response status" type="byte" />
			<field name="ResourcePackIds" type="list16">
				<field name="id" type="string"/>
			</field>
		</packet>

		<packet id="0x09" name="MCPE_TEXT">
			<field name="Type" type="byte" />
		</packet>

		<packet id="0x0a" name="MCPE_SET_TIME">
			<field name="Time" type="varint" />
		</packet>

		<packet id="0x0b" name="MCPE_START_GAME">

		</packet>

		<packet id="0x0c" name="MCPE_ADD_PLAYER">
			<field name="UUID" type="uuid" />
			<field name="Username" type="string" />
			<field name="Entity ID Self" type="varlong" />
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Platform Chat ID" type="string" />
			<field name="X" type="float" />
			<field name="Y" type="float" />
			<field name="Z" type="float" />
			<field name="Speed X" type="float" />
			<field name="Speed Y" type="float" />
			<field name="Speed Z" type="float" />
			<field name="Pitch" type="float" />
			<field name="Yaw" type="float" />
			<field name="Head Yaw" type="float" />
			<field name="Item" type="item" />
			<field name="Game Type" type="varint" />
			<field name="Metadata" type="metadataDictionary" />

			<field name="Flags" type="uvarint" />
			<field name="Command permission" type="uvarint" />
			<field name="Action Permissions" type="uvarint" />
			<field name="Permission Level" type="uvarint" />
			<field name="Custom stored permissions" type="uvarint" />
			<field name="User Id" type="long" />
			<field name="Links" type="entityLinks" />
			<field name="Device ID" type="string" />
			<field name="Device OS" type="int" />
		</packet>


		<!--
		TODO: Links
			count short
			loop
				link[0] long
				link[1] long
				link[2] byte
		TODO: Modifiers
			count int
				name string
				val1 float
				val2 float
		-->
		<packet id="0x0d" name="MCPE_ADD_ENTITY">
			<field name="Entity ID Self" type="varlong" />
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Entity Type" type="string" />
			<field name="X" type="float" />
			<field name="Y" type="float" />
			<field name="Z" type="float" />
			<field name="Speed X" type="float" />
			<field name="Speed Y" type="float" />
			<field name="Speed Z" type="float" />
			<field name="Pitch" type="float" />
			<field name="Yaw" type="float" />
			<field name="Head Yaw" type="float" />
			<field name="Attributes" type="listUVarInt" >
				<field name="Name" type="string" />
				<field name="Min Value" type="float" />
				<field name="Value" type="float" />
				<field name="Max Value" type="float" />
			</field>
			<field name="Metadata" type="metadataDirectory" />
			<field name="Links" type="entityLinks" />
		</packet>

		<packet id="0x0e" name="MCPE_REMOVE_ENTITY">
			<field name="Entity ID Self" type="varlong" />
		</packet>

		<packet id="0x0f" name="MCPE_ADD_ITEM_ENTITY">
			<field name="Entity ID Self" type="varlong" />
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="item" type="item" />
			<field name="X" type="float" />
			<field name="Y" type="float" />
			<field name="Z" type="float" />
			<field name="Speed X" type="float" />
			<field name="Speed Y" type="float" />
			<field name="Speed Z" type="float" />
			<field name="Metadata" type="metadataDictionary" />
			<field name="Is From Fishing" type="bool" />
		</packet>

		<packet id="0x11" name="MCPE_TAKE_ITEM_ENTITY">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Target" type="uvarlong" />
		</packet>

		<packet id="0x12" name="MCPE_MOVE_ENTITY">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Flags" type="byte" />
			<field name="Position" type="PlayerLocation" />
			<!--<field name="On Ground" type="bool" />
		<field name="Teleport" type="bool" />-->
		</packet>

		<packet id="0x13" name="MCPE_MOVE_PLAYER">

			<enums id="Mode">
				<enum id="Normal" value="0" />
				<enum id="Reset" value="1" />
				<enum id="Teleport" value="2" />
				<enum id="Rotation" value="3" />
			</enums>

			<!--
		These are sent at the end of the packet IF Telport mode is selected.
		-->
			<enums id="TeleportCause">
				<enum id="Unknown" value="0" />
				<enum id="Projectile" value="1" />
				<enum id="Chorus Fruit" value="2" />
				<enum id="Command" value="3" />
				<enum id="Behavior" value="4" />
				<enum id="Count" value="5" />
			</enums>

			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="X" type="float" />
			<field name="Y" type="float" />
			<field name="Z" type="float" />
			<field name="Pitch" type="float" />
			<field name="Yaw" type="float" />
			<field name="Head Yaw" type="float" />
			<field name="Mode" type="byte" />
			<field name="On Ground" type="bool" />
			<field name="Other Runtime Entity ID" type="uvarlong" />
		</packet>

		<packet id="0x14" name="MCPE_RIDER_JUMP">
			<field name="Unknown" type="varint" />
		</packet>

		<packet id="0x15" name="MCPE_UPDATE_BLOCK">
			<enums id="Flags">
				<enum id="None" value="0" />
				<enum id="Neighbors" value="1" />
				<enum id="Network" value="2" />
				<enum id="Nographic" value="4" />
				<enum id="Priority" value="8" />

				<enum id="All" value="(Neighbors | Network)" />
				<enum id="All Priority" value="(All | Priority)" />
			</enums>
			<field name="Coordinates" type="posVarint" />
			<field name="Block Runtime ID" type="uvarint" />
			<field name="Block Priority" type="uvarint" />
			<field name="Storage" type="uvarint" />
		</packet>

		<packet id="0x16" name="MCPE_ADD_PAINTING">
			<field name="Entity ID Self" type="varlong" />
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Coordinates" type="posVarint" />
			<field name="Direction" type="varint" />
			<field name="Title" type="string" />
		</packet>

		<packet id="0x17" name="MCPE_TICK_SYNC">
			<field name="Request Time" type="long" />
			<field name="Response Time" type="long" />
		</packet>

		<packet id="0x18" name="MCPE_LEVEL_SOUND_EVENT_OLD">
			<field name="Sound ID" type="byte" />
			<field name="Position" type="posFloat" />
			<field name="Block Id" type="varint" />
			<field name="Entity Type" type="varint" />
			<field name="Is baby mob" type="bool" />
			<field name="Is global" type="bool" />
		</packet>

		<packet id="0x19" name="MCPE_LEVEL_EVENT">
			<field name="Event ID" type="varint" />
			<field name="Position" type="posFloat" />
			<field name="Data" type="varint" />
		</packet>

		<packet id="0x1a" name="MCPE_BLOCK_EVENT">
			<field name="Coordinates" type="posVarint" />
			<field name="Case 1" type="varint" />
			<field name="Case 2" type="varint" />
		</packet>

		<packet id="0x1b" name="MCPE_ENTITY_EVENT">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Event ID" type="byte" />
			<field name="Data" type="varint" />
		</packet>

		<packet id="0x1c" name="MCPE_MOB_EFFECT">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Event ID" type="byte" />
			<field name="Effect ID" type="varint" />
			<field name="Amplifier" type="varint" />
			<field name="Particles" type="bool" />
			<field name="Duration" type="varint" />
		</packet>

		<packet id="0x1d" name="MCPE_UPDATE_ATTRIBUTES">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Attributes" type="PlayerAttributes" />
			<field name="Tick" type="uvarlong" />
		</packet>

		<packet id="0x1e" name="MCPE_INVENTORY_TRANSACTION">
			<field name="Transaction" type="transaction" />
		</packet>

		<packet id="0x1f" name="MCPE_MOB_EQUIPMENT">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="item" type="item" />
			<field name="Slot" type="byte" />
			<field name="Selected Slot" type="byte" />
			<field name="Windows Id" type="byte" />
		</packet>

		<packet id="0x20" name="MCPE_MOB_ARMOR_EQUIPMENT">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Helmet" type="item" />
			<field name="Chestplate" type="item" />
			<field name="Leggings" type="item" />
			<field name="Boots" type="item" />
		</packet>

		<packet id="0x21" name="MCPE_INTERACT">
			<enums id="Actions">
				<enum id="Right click" value="1" />
				<enum id="Left click" value="2" />
				<enum id="Leave vehicle" value="3" />
				<enum id="Mouse over" value="4" />
				<enum id="Open NPC" value="5" />
				<enum id="Open inventory" value="6" />
			</enums>

			<field name="Action ID" type="byte" />
			<field name="Target Runtime Entity ID" type="uvarlong" />
		</packet>

		<packet id="0x22" name="MCPE_BLOCK_PICK_REQUEST">
			<field name="X" type="varint" />
			<field name="Y" type="varint" />
			<field name="Z" type="varint" />
			<field name="Add User Data" type="bool" />
			<field name="Selected Slot" type="byte" />
		</packet>

		<packet id="0x23" name="MCPE_ENTITY_PICK_REQUEST">
			<field name="Runtime Entity ID" type="ulong" />
			<field name="Selected Slot" type="byte" />
			<field name="Add User Data" type="bool" />
		</packet>

		<packet id="0x24" name="MCPE_PLAYER_ACTION">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Action ID" type="varint" />
			<field name="Coordinates" type="posVarint" />
			<field name="Face" type="varint" />
		</packet>

		<packet id="0x26" name="MCPE_HURT_ARMOR">
			<field name="Cause" type="VarInt" />
			<field name="Health" type="varint" />
			<field name="Armor slot flags" type="uvarlong" />
		</packet>

		<packet id="0x27" name="MCPE_SET_ENTITY_DATA">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Metadata" type="metadataDictionary" />
			<field name="Tick" type="uvarlong" />
		</packet>

		<packet id="0x28" name="MCPE_SET_ENTITY_MOTION">
			<field name="Runtime Entity ID" type="uvarlong" />
			<field name="Velocity" type="posFloat" />
		</packet>

		<packet id="0x29" name="MCPE_SET_ENTITY_LINK">
			<enums id="Link Actions">
				<enum id="Remove" value="0" />
				<enum id="Ride" value="1" />
				<enum id="Passenger" value="2" />
			</enums>
			<field name="Ridden ID" type="varlong" />
			<field name="Rider ID" type="varlong" />
			<field name="Link Type" type="byte" />
			<field name="Unknown" type="byte" />
		</packet>

		<packet id="0x2a" name="MCPE_SET_HEALTH">
			<field name="Health" type="varint" />
		</packet>

		<packet id="0x2b" name="MCPE_SET_SPAWN_POSITION">
			<field name="Spawn Type" type="varint" />
			<field name="Coordinates" type="posVarint" />
			<field name="Dimension" type="varint" />
			<field name="Unknown coordinates" type="posVarint" />
		</packet>

		<packet id="0x2c" name="MCPE_ANIMATE">
			<field name="Action ID" type="varint" />
			<field name="Runtime Entity ID" type="uvarlong" />
		</packet>

		<packet id="0x2d" name="MCPE_RESPAWN">
			<enums id="Respawn State">
				<enum id="Search" value="0" />
				<enum id="Ready" value="1" />
				<enum id="Client Ready" value="2" />
			</enums>
			<field name="X" type="float" />
			<field name="Y" type="float" />
			<field name="Z" type="float" />
			<field name="State" type="byte" />
			<field name="Runtime Entity ID" type="uvarlong" />
		</packet>

		<packet id="0x2e" name="MCPE_CONTAINER_OPEN">
			<field name="Window ID" type="byte" />
			<field name="Type" type="byte" />
			<field name="Coordinates" type="posVarint" />
			<field name="Runtime Entity ID" type="varlong" />
		</packet>

		<packet id="0x2f" name="MCPE_CONTAINER_CLOSE">
			<field name="Window ID" type="byte" />
			<field name="Server" type="bool" />
		</packet>

		<packet id="0x30" name="MCPE_PLAYER_HOTBAR">
			<field name="Selected Slot" type="uvarint" />
			<field name="Window ID" type="byte" />
			<field name="Select Slot " type="bool" />
		</packet>

		<packet id="0x31" name="MCPE_INVENTORY_CONTENT">
			<field name="Inventory Id" type="uvarint" />
			<field name="Input" type="itemStacks" />
		</packet>

		<packet id="0x32" name="MCPE_INVENTORY_SLOT">
			<field name="Inventory Id" type="uvarint" />
			<field name="Slot" type="uvarint" />
			<field name="item" type="item" />
		</packet>

		<packet id="0x33" name="MCPE_CONTAINER_SET_DATA">
			<field name="Window ID" type="byte" />
			<field name="Property" type="varint" />
			<field name="Value" type="varint" />
		</packet>

		<packet id="0x34" name="MCPE_CRAFTING_DATA">
			<field name="Recipes" type="Recipes" />
			<field name="Potion type recipes" type="PotionTypeRecipe[]" />
			<field name="potion container recipes" type="PotionContainerChangeRecipe[]" />
			<field name="Material reducer recipes" type="MaterialReducerRecipe[]" />
			<field name="Is Clean" type="bool" />
		</packet>

		<packet id="0x35" name="MCPE_CRAFTING_EVENT">
			<enums id="Recipe Types">
				<enum id="Shapeless" value="0" />
				<enum id="Shaped" value="1" />
				<enum id="Furnace" value="2" />
				<enum id="Furnace data" value="3" />
				<enum id="Multi" value="4" />
				<enum id="Shulker box" value="5" />
				<enum id="Chemistry Shapeless" value="6" />
				<enum id="Chemistry Shaped" value="7" />
			</enums>

			<field name="Window ID" type="byte" />
			<field name="Recipe Type" type="varint" />
			<field name="Recipe ID" type="UUID" />
			<field name="Input" type="itemStacks" />
			<field name="Result" type="itemStacks" />
		</packet>

		<packet id="0x36" name="MCPE_GUI_DATA_PICK_item">

		</packet>

		<enums id="Adventure Flags">
			<enum id="MayFly" value="0x40" />
			<enum id="NoClip" value="0x80" />
